                              <string notr="true">/* QSpinBox整体样式 */
QSpinBox {
    border: 1px solid rgb(192, 192, 192); /* 边框颜色 */
    border-radius: 1px;                  /* 圆角 */
    padding: 2px 5px;                    /* 内边距 */
    color: rgb(64, 64, 64);              /* 字体颜色 */
}

/* 鼠标悬停时的样式 */
QSpinBox:hover {
    border: 1px solid rgb(160,160,160); /* 边框变暗 */
}

/* 禁用状态的样式 */
QSpinBox:disabled {
    background: rgb(224, 224, 224);      /* 灰色背景 */
    color: rgb(128, 128, 128);           /* 字体颜色变灰 */
    border: 2px solid rgb(192, 192, 192); /* 边框颜色保持一致 */
}

/* 上下按钮的样式 */
QSpinBox::up-button, QSpinBox::down-button {
    background: rgb(255, 255, 255);      /* 按钮背景颜色 */
    border: 1px solid rgb(192, 192, 192); /* 边框颜色 */
    margin: 0.5px;                         /* 按钮与边框的间距 */
    border-radius: 1px;                  /* 按钮圆角 */
}

/* 鼠标悬停时按钮的样式 */
QSpinBox::up-button:hover, QSpinBox::down-button:hover {
    background: rgb(242,242,242);      /* 按钮背景变暗 */
}

/* 按下按钮时的样式 */
QSpinBox::up-button:pressed, QSpinBox::down-button:pressed {
    background: rgb(160, 160, 160);      /* 按钮背景更暗 */
}

/* 上按钮的箭头样式 */
QSpinBox::up-arrow {
    image: url(../res/up-arrow.png); /* 替换为你的上箭头图标路径 */
}

/* 下按钮的箭头样式 */
QSpinBox::down-arrow {
    image: url(:/res/down-arrow.png); /* 替换为你的下箭头图标路径 */

}

